<%= simple_form_for(@link, remote: true) do |f| %>
  <%= f.error_notification %>
<div class = "row">
  <div class="form-inputs col-md-5 col-md-offset-3">
    <!-- <%= f.input :in_url %> :in_url needs to be randomly generated based on :out_url-->
    <%= f.input :out_url, required: false, input_html: {class: 'form-control input-lg', style: 'text-align:center;margin:0 auto;'}, label: false , placeholder: 'https://www.google.com/maps/some0location1'%>
  </div>

  <div class="form-actions ">
    <%= f.button :submit, 'Try it Out!', class: 'btn btn-lg btn-primary col-md-1', style: 'margin: 0 auto;' %>
  </div>
<% end %>
</div>